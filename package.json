{
  "name": "autoforma",
  "version": "2.6.0",
  "description": "AutoForma is a headless dynamic form builder based on React Hook Form",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://sajinael98.github.io/AutoForma",
  "repository": {
    "type": "git",
    "url": "https://github.com/sajinael98/AutoForma.git"
  },
  "bugs": {
    "url": "https://github.com/sajinael98/AutoForma/issues"
  },
  "license": "MIT",
  "keywords": [
    "autoforma",
    "autoform",
    "dynamic-form",
    "schema-form",
    "react-hook-form",
    "headless-form",
    "form-builder",
    "typescript",
    "react"
  ],
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-hook-form": "^7.69.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.69.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.0"
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "predeploy": "npm run storybook:build",
    "deploy-storybook": "gh-pages -d storybook-static"
  }
}
